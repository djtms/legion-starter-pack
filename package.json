{
  "name": "legion-starter-pack",
  "version": "0.0.20",
  "description": "Run some example testcases, or use this as a seed to start writing your own.",
  "scripts": {
    "start": "eslint testcase.js && node testcase.js",
    "test": "eslint testcase.js ./examples/** && npm run 001_simple && npm run 002_ticket",
    "001_simple": "node ./examples/001_simple.js",
    "002_ticket": "node ./examples/002_ticket.js",
    "clean": "rimraf node_modules"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "command-line-args": "^4.0.1",
    "command-line-usage": "^4.0.0",
    "legion": "git+https://github.com/lane-webperformance/legion.git#v0.0.20",
    "legion-io-fetch": "git+https://github.com/lane-webperformance/legion-io-fetch.git#v0.0.20",
    "legion-io-delay": "git+https://github.com/lane-webperformance/legion-io-delay.git#v0.0.20",
    "legion-obstacle-course": "git+https://github.com/lane-webperformance/legion-obstacle-course.git#v0.0.20"
  },
  "devDependencies": {
    "eslint": "^3.15.0",
    "rimraf": "^2.5.4"
  }
}
